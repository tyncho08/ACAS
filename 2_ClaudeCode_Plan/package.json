{
  "name": "@claudecode/cobol-analyzer",
  "version": "1.0.0",
  "description": "Accurate, evidence-based COBOL code analysis tool",
  "main": "index.js",
  "bin": {
    "cobol-analyze": "./cli.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "validate": "node scripts/validate.js",
    "analyze:example": "node cli.js analyze -s ../examples/cobol -o ./analysis-output"
  },
  "keywords": [
    "cobol",
    "code-analysis",
    "static-analysis",
    "legacy-code",
    "mainframe",
    "metrics",
    "complexity"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "better-sqlite3": "^9.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "eslint": "^8.49.0",
    "@types/node": "^20.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "*.js",
      "!cli.js",
      "!coverage/**",
      "!node_modules/**",
      "!test/**"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claudecode/cobol-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/claudecode/cobol-analyzer/issues"
  },
  "homepage": "https://github.com/claudecode/cobol-analyzer#readme"
}